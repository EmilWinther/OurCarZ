@page
@model OurCarZ.Pages.UserPages.LogInPageModel
@{
}
<div class="form-container">
    <div>
        @Model.Message
    </div>
    <form method="post">
        <div class="form-group">
            <label asp-for="Email" class="col-form-label "></label>
            <input asp-for="Email" placeholder="Enter Email" class="default-form" type="text" required />
        </div>
        <div class="form-group">
            <label asp-for="Password" class="col-form-label"></label>
            <input asp-for="Password" placeholder="Enter Email" class="default-form" type="password" required />
        </div>
        <div class="form-group">
            <input type="submit" class="ubutton" value="Submit">
        </div>
    </form>
</div>
